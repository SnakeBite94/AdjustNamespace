<platformUi:DialogWindow 
    x:Class="AdjustNamespace.Window.AdjustNamespaceWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:vsshell="clr-namespace:Microsoft.VisualStudio.Shell;assembly=Microsoft.VisualStudio.Shell.15.0"
    xmlns:platformUi="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.15.0"
    xmlns:ui="clr-namespace:AdjustNamespace.UI"
    xmlns:imaging="clr-namespace:Microsoft.VisualStudio.Imaging;assembly=Microsoft.VisualStudio.Imaging"
    xmlns:main="clr-namespace:AdjustNamespace"
    mc:Ignorable="d" 
    Title="Adjust namespaces..."
    Loaded="DialogWindow_Loaded"
    WindowStartupLocation="CenterOwner"
    Height="400" Width="700"
    d:DesignHeight="300" d:DesignWidth="400"
    ui:VsTheme.UseVsTheme="True"
    >
    
    <ContentControl
        x:Name="CenterContentControl"
        />
</platformUi:DialogWindow>
